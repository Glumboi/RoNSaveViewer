<ListBoxItem
    x:Class="RoNSaveViewer_WPF.CustomControls.DumpableListEntry"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:RoNSaveViewer_WPF.CustomControls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    MouseRightButtonDown="ListBoxItem_MouseRightButtonDown"
    mc:Ignorable="d">
    <ListBoxItem.ContextMenu>
        <ContextMenu x:Name="DumpContextMenu">
            <ContextMenu.Items>
                <MenuItem Header="Dump to .txt file" />
            </ContextMenu.Items>
        </ContextMenu>
    </ListBoxItem.ContextMenu>>
</ListBoxItem>